{
    "nDPId": {
        "logFilesLengthInSeconds": 60,
        "logFilesLengthInMB": 5,

        "debugLogs": {
            "detailedLog": false,
            "generateMasterLogFile": true,
            "masterLogFileDurationInMinutes": 10
        },

        "consoleOutput": {
            "detail_level": 1
        },

        "sockets": {
            "COLLECTOR_UNIX_SOCKET": "/tmp/ndpid-collector_unix.sock",
            "COLLECTOR_RECONNECT_INTERVAL_SEC": 3,
            "COLLECTOR_RECONNECT_TIMEOUT_SEC": 60
        },

        "output": {
            "sendToSocket":true,
            "writeToJsonFiles": true
        }
    }
}